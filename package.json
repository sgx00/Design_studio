{
  "name": "ai-design-platform",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered design generation platform for converting images to flats and generating final product images",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "dependencies": {
    "@google/genai": "^1.15.0",
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fabric": "^5.3.0",
    "fs-extra": "^11.1.1",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "ai",
    "design",
    "image-processing",
    "fashion",
    "product-design"
  ],
  "author": "AI Design Platform",
  "license": "MIT"
}
